<StackPane id="SPane" fx:id="SPane" maxHeight="600.0" maxWidth="900.0" minHeight="500.0" minWidth="800.0" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.homeController">
   <children>
