            </HBox>
         </bottom>
         <top>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="50.0" BorderPane.alignment="CENTER">
               <children>
      <!--            <Icon content="HELP_OUTLINE" layoutX="744.0" layoutY="36.0" />-->
                  <Label alignment="CENTER" contentDisplay="CENTER" onMouseClicked="#HomeClicked" opacity="0.54" prefHeight="50.0" prefWidth="100.0" style="-fx-background-color: white;" text="Home" textFill="#1e1e1e">
